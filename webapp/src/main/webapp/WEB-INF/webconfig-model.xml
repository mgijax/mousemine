<webconfig>

  <!-- =============================================================== -->
  <class className="org.intermine.model.bio.Allele">
    <headerconfig>
      <titles>
        <title mainTitles="symbol" numberOfMainTitlesToShow="1" subTitles="name"/>
      </titles>
    </headerconfig>
    <fields>
      <fieldconfig fieldExpr="symbol" />
      <fieldconfig fieldExpr="name"/>
      <fieldconfig fieldExpr="primaryIdentifier" showInSummary="true"  /> 
      <fieldconfig fieldExpr="alleleType" showInSummary="true" />
      <fieldconfig fieldExpr="feature.symbol" showInResults = "false" showInSummary="false" showInInlineCollection="false" label="Gene" displayer="/model/genelinkdisplayer.jsp" />
      <fieldconfig fieldExpr="attributeString" />
      <fieldconfig fieldExpr="inheritanceMode" showInSummary="false" />
      <fieldconfig fieldExpr="glTransmission" label="Transmission" showInSummary="false" />
      <fieldconfig fieldExpr="strainOfOrigin.name" showInSummary="false" label="Strain of Origin"/>
      <fieldconfig fieldExpr="description" showInResults="false" showInInlineCollection="false" showInSummary="false" doNotTruncate="true" escapeXml="false" />
      <fieldconfig fieldExpr="molecularNote" showInResults="false" showInInlineCollection="false" showInSummary="false" doNotTruncate="true" escapeXml="false" />
    </fields>
   <inlinelist>
      <table path="mutations.name" showInHeader="true" lineLength="200" />
      <table path="cellLines.name" showInHeader="true" lineLength="200" showLinksToObjects="true" />
      <table path="synonyms.value" showInHeader="true" lineLength="200" />
      <table path="earliestPublication.citation" label="Earliest Citation" showInHeader="true" lineLength="200" showLinksToObjects="true" />
    </inlinelist>
  </class>
  

