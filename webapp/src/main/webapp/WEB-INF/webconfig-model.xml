    description="Pathways enriched for genes in this list - data from Reactome"
    label="Pathways"
    startClass="Gene"
    startClassDisplay="primaryIdentifier"
    enrich="pathways.name"
    enrichIdentifier="pathways.identifier"
    constraints="organism.taxonId=[list], primaryIdentifier != null"
    typeClass="Gene"
    views="symbol, organism.name, pathways.identifier, pathways.name"
    />

<enrichmentwidgetdisplayer id="prot_dom_enrichment_for_gene"
    title="Protein Domain Enrichment"
    description="Protein Domains enriched for items in this list."
    label="Protein Domain"
    startClass="Gene"
    startClassDisplay="primaryIdentifier"
    enrich="proteins.proteinDomainRegions.proteinDomain.name"
    enrichIdentifier="proteins.proteinDomainRegions.proteinDomain.primaryIdentifier"
    constraints="organism.name=[list]"
    typeClass="Gene"
    views="secondaryIdentifier,symbol,primaryIdentifier,organism.name,
       proteins.proteinDomainRegions.proteinDomain.primaryIdentifier,proteins.proteinDomainRegions.proteinDomain.name"
    externalLink="http://www.ebi.ac.uk/interpro/IEntry?ac="
    />

<enrichmentwidgetdisplayer id="disease_enrichment_for_feature"
    title="Disease Enrichment"
    description="Disease Ontology terms enriched for items in this list."
    label="DO Term"
    startClass="SequenceFeature"
    typeClass="SequenceFeature"
    startClassDisplay="primaryIdentifier"
    enrich="ontologyAnnotations.ontologyTerm[DOTerm].parents.name"
    enrichIdentifier="ontologyAnnotations.ontologyTerm.parents.identifier"
    constraints="organism.name=[list],
	primaryIdentifier != null, 
	ontologyAnnotations.qualifier = null"
    views="symbol,name,primaryIdentifier,organism.name,
	ontologyAnnotations.ontologyTerm.identifier, 
	ontologyAnnotations.ontologyTerm.name,
	ontologyAnnotations.ontologyTerm.parents.identifier,
	ontologyAnnotations.ontologyTerm.parents.name"
    externalLink="http://www.informatics.jax.org/disease/"
    />

<enrichmentwidgetdisplayer id="mp_enrichment_for_feature"
